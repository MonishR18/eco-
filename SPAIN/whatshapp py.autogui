import pyautogui as pg
import time

# Giving Dealy to run program
print("program will run after 5 second")
time.sleep(10)
print("running")

# For loop
for i in range(1):
    # writing the messages
    pg.write("Hi, This is automatic random message generated by python")
    time.sleep(10) 
    # Seding the messages by pressing enter
    pg.press("20")